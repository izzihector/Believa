<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="ebay_feedback_wizard_view" model="ir.ui.view">
			<field name="name">ebay.feedback.wizard</field>
			<field name="model">ebay.feedback.wizard</field>
			<field name="arch" type="xml">
				<form string="form view">
					<group string='FeedBack Replay Details'>
						<group>
							<field name='response_text' />
							<field name='response_type' />
						</group>
					</group>
					<footer>
						<button name='update_changes' string="Update" type="object"
							class="oe_highlight" />
						or
						<button string="Close" class="oe_link" special="cancel" />
					</footer>
				</form>
			</field>
		</record>
	</data>
</odoo>